{
  "name": "eslint-plugin-preferred-import",
  "version": "1.0.7",
  "description": "Lint rule to autofix unpreferred imports",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "tsc -p tsconfig.build.json --build --clean",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r0npark/eslint-plugin-preferred-import.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "import",
    "relative",
    "modules",
    "autofix"
  ],
  "author": "Sang Min Park (r0npark)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r0npark/eslint-plugin-preferred-import/issues",
    "email": "ronpark.dev@gmail.com"
  },
  "homepage": "https://github.com/r0npark/eslint-plugin-preferred-import#readme",
  "devDependencies": {
    "@types/eslint": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.1",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.27.1"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  }
}
